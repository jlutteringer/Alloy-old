<?xml version="1.0" encoding="UTF-8"?>
<Configuration>
	<Appenders>
		<Console name="STDOUT">
			<PatternLayout pattern="%d{yyyy-MM-dd HH:mm:ss,SSS} - %5p (%F:%L) - %m%n" />
		</Console>
	</Appenders>
	<Loggers>
		<Logger name="org.vault" level="debug" />
		<Root level="warn">
			<AppenderRef ref="STDOUT"/>
		</Root>
	</Loggers>
</Configuration>